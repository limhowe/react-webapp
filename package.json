{
  "name": "dp-web-app",
  "version": "0.0.1",
  "description": "React/Redux/React-router isomorphic app for DP",
  "main": "index.js",
  "scripts": {
    "dev": "node ./bin/server",
    "start": "cross-env NODE_ENV=production node ./bin/server",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir lib --copy-files",
    "build:client": "cross-env NODE_ENV=production webpack --progress",
    "clean": "rimraf dist lib",
    "check": "npm run lint && npm run test",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint src/**/*.scss --syntax scss",
    "test": "cross-env NODE_ENV=test mocha mocha.config.js",
    "flow": "flow",
    "flow:stop": "flow stop"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:cmodels/dp-web-app.git"
  },
  "keywords": [
    "isomorphic",
    "react",
    "redux",
    "react-router",
    "react-toolbox"
  ],
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "breakpoint-sass": "^2.7.0",
    "classnames": "^2.2.5",
    "country-data": "0.0.31",
    "dotenv": "^2.0.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "highcharts": "^5.0.6",
    "i18next": "~3.4.4",
    "immutability-helper": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0-alpha.5",
    "koa-static": "^3.0.0",
    "localforage": "^1.4.3",
    "moment": "^2.17.0",
    "moment-timezone": "^0.5.10",
    "proj4": "^2.3.15",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^0.14.8",
    "react-countup": "^1.3.0",
    "react-dom": "^15.3.0",
    "react-dropzone": "^3.7.3",
    "react-helmet": "~3.1.0",
    "react-i18next": "^1.10.1",
    "react-onclickoutside": "^5.7.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.3",
    "react-router-redux": "^4.0.6",
    "react-toolbox": "^1.2.4",
    "recharts": "^0.18.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.13.0",
    "redux-form": "^6.1.1",
    "redux-fsa-thunk": "0.0.2",
    "redux-persist": "^4.0.0-alpha7",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "superagent": "^2.3.0",
    "webpack-isomorphic-tools": "^2.5.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0-1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.0",
    "eslint-plugin-flowtype": "^2.7.0",
    "eslint-plugin-react": "^6.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "flow-bin": "^0.30.0",
    "glob": "^7.0.5",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "sinon": "^2.0.0-pre.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
